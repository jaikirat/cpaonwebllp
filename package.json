{
  "name": "cpaonwebllp",
  "version": "0.1.0",
  "description": "Next.js TypeScript project with Cloudflare Pages deployment pipeline for CPA On Web LLP",
  "type": "module",
  "private": true,
  "homepage": "https://cpaonweb.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:contract": "node --test tests/contract/*.spec.js",
    "test:contract:cloudflare": "node --test tests/contract/test_cloudflare_pages.spec.js",
    "test:integration": "jest tests/integration",
    "test:integration:domain": "jest tests/integration/custom-domain-setup.test.ts",
    "test:project-creation": "jest tests/integration/project-creation.test.js",
    "test:repository": "jest tests/integration/repository-setup.test.ts",
    "test:production": "jest tests/integration/production-deployment.test.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:bundle": "jest tests/bundle/size-analysis.test.ts --testTimeout=180000",
    "test:bundle:watch": "jest tests/bundle/size-analysis.test.ts --watch --testTimeout=180000",
    "analyze:bundle": "npm run build && npm run test:bundle",
    "test:cf-pages": "node --test tests/integration/cloudflare-pages-integration.test.js",
    "test:integration:node": "node --test tests/integration/*.test.js",
    "test:visual": "playwright test",
    "test:visual:ui": "playwright test --ui",
    "test:visual:headed": "playwright test --headed",
    "test:visual:debug": "playwright test --debug",
    "test:visual:update": "playwright test --update-snapshots",
    "test:visual:chromium": "playwright test --project=chromium",
    "test:visual:firefox": "playwright test --project=firefox",
    "test:visual:webkit": "playwright test --project=webkit",
    "test:visual:mobile": "playwright test --project='Mobile Chrome' --project='Mobile Safari'"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "lucide-react": "^0.544.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node-fetch": "^2.6.11",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "bundlesize2": "^0.0.35",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "filesize": "^11.0.2",
    "gzip-size": "^7.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-environment-node": "^29.7.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "playwright": "^1.55.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.4",
    "webpack": "^5.101.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1"
  }
}
