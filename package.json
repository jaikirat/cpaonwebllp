{
  "name": "cpaonwebllp",
  "version": "0.1.0",
  "description": "Next.js TypeScript project with Cloudflare Pages deployment pipeline for CPA On Web LLP",
  "type": "module",
  "private": true,
  "homepage": "https://cpaonweb.com",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:contract": "node --test tests/contract/*.spec.js",
    "test:contract:cloudflare": "node --test tests/contract/test_cloudflare_pages.spec.js",
    "test:integration": "jest tests/integration",
    "test:integration:domain": "jest tests/integration/custom-domain-setup.test.ts",
    "test:project-creation": "jest tests/integration/project-creation.test.js",
    "test:repository": "jest tests/integration/repository-setup.test.ts",
    "test:production": "jest tests/integration/production-deployment.test.ts",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:cf-pages": "node --test tests/integration/cloudflare-pages-integration.test.js",
    "test:integration:node": "node --test tests/integration/*.test.js"
  },
  "dependencies": {
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "@types/node-fetch": "^2.6.11",
    "tailwindcss": "^4",
    "ts-jest": "^29.1.2",
    "typescript": "^5"
  }
}
